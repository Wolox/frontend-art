{
  "plugins": [
    "transform-object-rest-spread",
    "dynamic-import-webpack",
    "syntax-dynamic-import",
    ["prismjs", {
      "languages": ["markup", "javascript", "scss"],
      "plugins": ["line-numbers"],
      "theme": "default",
      "css": true
    }]
  ],
  "presets": [
    ["env", {
      "targets": [
        "last 2 versions", "safari >= 7", "not ie < 9"
      ]
    }]
  ]
}
