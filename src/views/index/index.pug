doctype html
html
  head
    title Frontend Cookbook
    meta(charset='UTF-8')
    meta(name='viewport' content='width=device-width, initial-scale=1')
    link(rel='preload' href='./index.js' as='script')
    link(rel='preload' href='./vendor.js' as='script')
    link(rel='preload' href='styles.index.css' as='style')
    link(rel='icon' type='image/png' href='/assets/favicon.ico')
    link(rel='stylesheet' href='styles.index.css')
    script(src='./vendor.js')
    link(rel='stylesheet', type='text/css', href='build/application.css')
    script(src='../scripts/progress.js')
  body
    #app.container(v-cloak)
      sidebar(@list='changeCurrentList')
      .feed-content
        h2.title
          | {{ currentList }}
        card-list(to-show='spinners')
    script(src='./index.js')
